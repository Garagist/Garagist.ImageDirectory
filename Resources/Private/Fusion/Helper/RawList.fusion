prototype(Garagist.ImageDirectory:Helper.RawList) {
    @class = 'Garagist\\ImageDirectory\\Fusion\\RawListImplementation'

    startingPoint = ${site}
    renderHiddenInIndex = true
    # Can be document or asset
    sortBy = ${null}

    ignoreFilenames = ${Configuration.setting('Garagist.ImageDirectory.ignoreFilenames')}

    # Comma seperated list
    mediaTypes = 'image/*,video/*'

    # Comma seperated list
    filter = 'Neos.Neos:Document'
}
