prototype(Garagist.ImageDirectory:RawList) {
    @class = 'Garagist\\ImageDirectory\\Fusion\\RawListImplementation'

    startingPoint = ${site}
    renderHiddenInIndex = true

    # Comma seperated list
    mediaTypes = 'image/*,video/*'

    # Comma seperated list
    filter = 'Neos.Neos:Document,!Neos.Neos:Shortcut'
}
